import java.util.Scanner;

public class uktc{
    public static void main(String[]args){
        Scanner scan = new Scanner(System.in);
        
        int n = scan.nextInt();

        for (int i = 0; i < n ; i++) {
            for (int j = 0; j < n ; j++) {
                System.out.println("*");
            }
        }
    }
}





import java.util.Arrays;
import java.util.Collections;
import java.util.Scanner;
 
public class App4 {
    public static void print(){
        System.out.println("Hi");
    }
 
    public static int sum(int n,int n2){
        return n + n2;
    }
 
    public static void repeatSymbol(int n, String str){
        for (int i = 0; i < n; i++) {
            System.out.print(str);
        }
        System.out.println();
    }
 
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
 
        for (int i = 0; i < n; i++) {
            repeatSymbol(n,"*");
        }
    }
}
